{
  "name": "nexusmarket-backend",
  "version": "1.0.0",
  "description": "Multi-vendor e-commerce platform backend API",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec node --loader ts-node/esm src/server.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node --loader ts-node/esm src/scripts/seed.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "e-commerce",
    "multi-vendor",
    "typescript",
    "express",
    "prisma",
    "supabase",
    "stripe"
  ],
  "author": "Dakota King",
  "license": "MIT",
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.0",
    "@prisma/client": "^6.15.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/node": "^24.3.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "prisma": "^6.15.0",
    "rate-limit-redis": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.2.0-preview-contacts.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.7.0",
    "stripe": "^18.5.0",
    "svix": "^1.11.0",
    "typescript": "^5.6.0",
    "winston": "^3.11.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "jest": "^30.1.1",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
